{
    "manifest_version": 3,
    "name": "收藏",
    "version": "1.0",
    "description": "Right-click on an image to search it on Yandex.",
    "permissions": [
        "contextMenus",
        "notifications",
        "storage",
        "tabs",
        "activeTab"
    ],
